{
  "name": "lara-ananas/pikaphp",
  "description": "PikaPHP: The Small, Swift, and Powerful Toolkit",
  "keywords": [
    "php",
    "storage",
    "file",
    "filesystem"
  ],
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "homepage": "https://github.com/lara-ananas/PikaPHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "lara-ananas",
      "email": "lara@mailo.com",
      "homepage": "https://github.com/lara-ananas",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-pdo_mysql": "*",
    "ext-zlib": "*",
    "catfan/medoo": "^2.1"
  },
  "require-dev": {},
  "suggest": {},
  "autoload": {
    "psr-4": {
      "PikaPHP\\": "src/PikaPHP"
    }
  },
  "autoload-dev": {}
}